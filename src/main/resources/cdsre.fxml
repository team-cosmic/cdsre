<SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="773.0" prefWidth="814.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cdsre.CDSREController">
